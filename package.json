	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"test": "vitest --silent",
		"test:watch": "vitest --silent --watch",
		"test:ci": "vitest run --silent",
		"test:coverage": "vitest run --coverage --watch=false",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:headed": "playwright test --headed",
		"test:audit": "npx lighthouse http://localhost:4173 --view --output=html --output-path=./lighthouse-report-mobile.html",
		"test:audit:desktop": "npx lighthouse http://localhost:4173 --view --preset=desktop --output=html --output-path=./lighthouse-report-desktop.html"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
